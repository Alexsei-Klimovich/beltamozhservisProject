            <xs:element minOccurs="0" ref="csdo:DocSeriesId"/>
            <xs:element ref="csdo:DocId"/>
            <xs:element minOccurs="0" ref="csdo:DocCreationDate"/>
            <xs:element minOccurs="0" ref="csdo:BusinessEntityId">
