        <xs:annotation>
            <xs:documentation>Сведения о документе об образовании (квалификации, обучении)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00085: Документ об образовании (Документ об образовании_ Детали. Тип) -->
    <xs:complexType name="AcademicCertificateDetailsType">
