        <xs:annotation>
            <xs:documentation>кодовое обозначение вида графика работы организации (подразделения)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00083: Код времени работы организации (подразделения) (Время работы. Код) -->
    <xs:element name="WorkTimeCode" type="csdo:Code2Type">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение времени работы организации (подразделения)</xs:documentation>
        </xs:annotation>
    </xs:element>
