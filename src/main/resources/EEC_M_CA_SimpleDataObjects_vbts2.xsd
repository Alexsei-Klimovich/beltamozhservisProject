        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.01172: Признак наличия оружия и (или) боеприпасов (Признак наличия оружия и (или) боеприпасов. Индикатор) -->
    <xs:element name="WeaponIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак наличия оружия и (или) боеприпасов</xs:documentation>
        </xs:annotation>
    </xs:element>
