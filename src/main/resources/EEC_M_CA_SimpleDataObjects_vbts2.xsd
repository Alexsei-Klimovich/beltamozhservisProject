<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:casdo="urn:EEC:M:CA:SimpleDataObjects:vbts2"
           xmlns:bdt="urn:EEC:M:BaseDataTypes:vbts6" xmlns:csdo="urn:EEC:M:SimpleDataObjects:vbts5"
           targetNamespace="urn:EEC:M:CA:SimpleDataObjects:vbts2" elementFormDefault="qualified" version="1.5.2">
    <xs:import namespace="urn:EEC:M:BaseDataTypes:vbts6" schemaLocation="EEC_M_BaseDataTypes_vbts6.xsd"/>
    <xs:import namespace="urn:EEC:M:SimpleDataObjects:vbts5" schemaLocation="EEC_M_SimpleDataObjects_vbts5.xsd"/>
    <!-- M.CA.SDE.00592: Код типа свидетельства (Уполномоченный экономический оператор_ Тип свидетельства. Код) -->
    <xs:element name="AEORegistryKindCode" type="csdo:Code1Type">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение типа свидетельства уполномоченного экономического оператора
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.SDT.00069: Имя (Имя. До 40 символов. Тип) -->
    <xs:simpleType name="Name40Type2">
        <xs:annotation>
            <xs:documentation>Слово или фраза, обозначающая отличительную особенность объекта, явления или процесса,
                содержащая не более 40 символов
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:NameType">
            <xs:minLength value="1"/>
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.CA.SDE.00516: Идентификатор применения (Идентификатор применения. Идентификатор) -->
    <xs:element name="AIId" type="casdo:AIIdType">
        <xs:annotation>
            <xs:documentation>Цифровой идентификатор блока информации, содержащейся в средстве идентификации
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00208: Идентификатор применения (Идентификатор применения_ Идентификатор. Тип) -->
    <xs:simpleType name="AIIdType">
        <xs:annotation>
            <xs:documentation>Строка символов, определяющая индентификатор</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:IdentifierType">
            <xs:pattern value="\d{2,4}"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.CA.SDE.01159: Признак наличия необычного уровня заболеваемости (Признак наличия необычного уровня заболеваемости. Индикатор) -->
    <xs:element name="AbnormalDiseasePersonIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак наличия необычного уровня заболеваемости</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00153: Количество добавочных листов (Добавочный лист. Количество) -->
    <xs:element name="AddPageQuantity" type="csdo:Quantity6Type">
        <xs:annotation>
            <xs:documentation>Количество добавочных листов</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00362: Сумма иных расходов, связанных с производством товаров (Сумма иных расходов, связанных с производством товаров. Денежная сумма) -->
    <xs:element name="AddProductionValueAmount" type="casdo:PaymentAmountWithCurrencyType">
        <xs:annotation>
            <xs:documentation>Сумма расходов, связанных с производством товаров</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.01132: Признак посещения зараженной территории (Транспортное средство_ Признак посещения зараженной территории. Индикатор) -->
    <xs:element name="AffectedAreaVisitIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак посещения зараженной территории</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00539: Код вида уровня маркировки (Уровень маркировки товаров_ Вид. Код) -->
    <xs:element name="AggregationKindCode" type="csdo:Code1Type">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение уровня маркировки</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.01168: Идентификатор авиакомпании (Идентификатор авиакомпании. Идентификатор) -->
    <xs:element name="AirlineId" type="casdo:Id3Type">
        <xs:annotation>
            <xs:documentation>Идентификатор авиакомпании</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00196: Денежная сумма с буквенным кодом валюты (Денежная сумма с буквенным кодом валюты_ Денежная сумма. Тип) -->
    <xs:complexType name="AmountCurrencyType">
        <xs:annotation>
            <xs:documentation>Числовое значение, представляющее количество денежных единиц, дополненное буквенным кодом
                валюты и указанием на классификатор валют
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="bdt:AmountType">
                <xs:attribute name="currencyCode" type="csdo:CurrencyCodeV3Type" use="required">
                    <xs:annotation>
                        <xs:documentation>Кодовое обозначение валюты</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="currencyCodeListId" type="csdo:ReferenceDataIdType" use="optional">
                    <xs:annotation>
                        <xs:documentation>Идентификатор справочника (классификатора)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <!-- M.CA.SDE.01142: Признак наличия противоэпидемического имущества (Признак наличия противоэпидемического имущества. Индикатор) -->
    <xs:element name="AntiepidemicEquipmentIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак наличия противоэпидемического имущества</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00065: Код назначения товара (Назначение товара. Код) -->
    <xs:element name="AppointmentCode" type="csdo:Code1Type">
        <xs:annotation>
            <xs:documentation>кодовое обозначение назначения транспортного средства: С – коммерческое, L – личное.
                Применяется для Российской Федерации
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00372: Расчетная стоимость (Расчетная стоимость. Денежная сумма) -->
    <xs:element name="ApproximateValueAmount" type="casdo:PaymentAmountWithCurrencyType">
        <xs:annotation>
            <xs:documentation>Расчетная стоимость товаров</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00363: Признак близости стоимости к проверочной величине (Признак близости стоимости к проверочной величине. Индикатор) -->
    <xs:element name="ApproximateValueIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак близости стоимости к проверочной величине</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00431: Дата и время прибытия (Дата и время прибытия. Дата и время) -->
    <xs:element name="ArrivalDateTime" type="bdt:DateTimeType">
        <xs:annotation>
            <xs:documentation>Дата и время прибытия в таможенный орган</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00004: Наименование места прибытия (убытия) товаров (Место прибытия (убытия) товаров. Имя) -->
    <xs:element name="ArrivePointName" type="csdo:Name250Type">
        <xs:annotation>
            <xs:documentation>Наименование места прибытия (убытия) товаров в соответствии с перечнем мест прибытия
                (убытия) товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00821: Кодовое обозначение первого элемента места прибытия (убытия) товаров (Код места прибытия (убытия) товаров_ Код первого элемента. Код) -->
    <xs:element name="ArrivePointPADCode" type="casdo:ArrivePointPADCodeType">
        <xs:annotation>
            <xs:documentation>кодовое обозначение первого элемента кодового обозначения места прибытия (убытия)
                товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00821: Первый элемент кодового обозначения места прибытия (убытия) товаров (Первый элемент кодового обозначения места прибытия (убытия) товаров_ Код. Тип) -->
    <xs:simpleType name="ArrivePointPADCodeType">
        <xs:annotation>
            <xs:documentation>строка символов, представляющая краткое буквенное обозначение первого элемента кодового
                обозначения места прибытия (убытия) товаров
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:pattern value="PAD"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.CA.SDE.00820: Порядковый номер места прибытия (убытия) товаров (Код места прибытия (убытия) товаров_ Порядковый номер места прибытия (убытия). Код) -->
    <xs:element name="ArrivePointSequenceCode" type="casdo:Number2CodeType">
        <xs:annotation>
            <xs:documentation>кодовое обозначение места прибытия (убытия) товаров в пределах одного таможенного органа
                (четвертый элемент кодового обозначения места прибытия (убытия) товаров)
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00353: Сумма расходов на строительство, возведение, сборку, монтаж (Сделка_ Сумма расходов на строительство, возведение, сборку, монтаж. Денежная сумма) -->
    <xs:element name="AssemblyValueAmount" type="casdo:PaymentAmountWithCurrencyType">
        <xs:annotation>
            <xs:documentation>Расходы на строительство, возведение, сборку, монтаж, обслуживание или оказание
                технического содействия
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00497: Регион регистрации транспортного средства (Регион регистрации транспортного средства. Код) -->
    <xs:element name="AutoRegionCode" type="casdo:AutoRegionCodeType">
        <xs:annotation>
            <xs:documentation>Код региона</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00122: Код региона регистрации транспортного средства (Регион регистрации транспортного средства_ Код. Тип) -->
    <xs:simpleType name="AutoRegionCodeType">
        <xs:annotation>
            <xs:documentation>строка символов, представляющая цифровое обозначение региона (регистрационный номер ТС,
                паспорта транспортного средства, паспорта шасси транспортного средства)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:pattern value="\d{2,3}"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.CA.SDE.00179: Количество единиц в валюте (Количество единиц в валюте. Количество) -->
    <xs:element name="BYCurrencyQuantity" type="csdo:Quantity6Type">
        <xs:annotation>
            <xs:documentation>Количество единиц валюты, для которой указан курс</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00380: Количество единиц в валюте (косвенный платеж) (Косвенный платеж_ Количество единиц в валюте. Количество) -->
    <xs:element name="BYIndirectCurrencyQuantity" type="csdo:Quantity6Type">
        <xs:annotation>
            <xs:documentation>Количество единиц валюты, для которой указан курс (косвенный платеж)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00104: Код признака журнала регистрации (Признак журнала регистрации_ Код. Тип) -->
    <xs:simpleType name="BYSignCodeType">
        <xs:annotation>
            <xs:documentation>строка символов, представляющая краткое условное обозначение признака журнала регистрации
                для Республики Беларусь. Варианты кода: Ф, Д, 7
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.CA.SDE.00318: Код базового метода определения таможенной стоимости (Базовый метод определения таможенной стоимости. Код) -->
    <xs:element name="BaseValuationMethodCode" type="casdo:ValuationMethodCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение базового метода определения таможенной стоимости</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00681: Порядковый номер накладной (Накладная. Порядковый номер) -->
    <xs:element name="BillOfLandingOrdinal" type="csdo:Ordinal3Type">
        <xs:annotation>
            <xs:documentation>Порядковый номер накладной</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00006: Код пункта пропуска (Пункт пропуска. Код) -->
    <xs:element name="BorderCheckPointCode" type="casdo:BorderCheckPointCodeType">
        <xs:annotation>
            <xs:documentation>кодовое обозначение пункта пропуска</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00010: Код пункта пропуска (Пункт пропуска_ Код. Тип) -->
    <xs:simpleType name="BorderCheckPointCodeType">
        <xs:annotation>
            <xs:documentation>строка символов, представляющая цифровое обозначение пункта пропуска</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:minLength value="1"/>
            <xs:maxLength value="18"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.CA.SDE.00007: Наименование пункта пропуска (Пункт пропуска. Имя) -->
    <xs:element name="BorderCheckPointName" type="csdo:Name250Type">
        <xs:annotation>
            <xs:documentation>Наименование сопредельного пункта пропуска</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00257: Код таможенного органа места убытия (Таможенный орган места убытия. Код) -->
    <xs:element name="BorderCustomsOfficeCode" type="csdo:CustomsOfficeCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение таможенного органа места убытия</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00008: Расстояние до пункта пропуска (Пункт пропуска_ Расстояние от границы. Измерение) -->
    <xs:element name="BorderDistanceMeasure" type="casdo:FractionNumber24MeasureType">
        <xs:annotation>
            <xs:documentation>Удаленность пункта пропуска от таможенной границы Союза</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00158: Признак филиала организации (Организация_ Признак филиала организации. Код) -->
    <xs:element name="BranchFlagCode" type="csdo:Code1Type">
        <xs:annotation>
            <xs:documentation>кодовое обозначение признака филиала организации</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00340: Сумма вознаграждений агенту (посреднику), брокеру (Сделка_ Сумма вознаграждений посреднику (агенту), брокеру. Денежная сумма) -->
    <xs:element name="BrokerageAmount" type="casdo:PaymentAmountWithCurrencyType">
        <xs:annotation>
            <xs:documentation>Расходы покупателя на вознаграждение агентам (посредникам) и вознаграждение брокерам
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00197: Сумма платежа c указанием валюты (Платеж c указанием кода валюты_ Денежная сумма. Тип) -->
    <xs:complexType name="CAAmountWithCurrencyType">
        <xs:annotation>
            <xs:documentation>Количество денежных единиц</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:restriction base="casdo:AmountCurrencyType">
                <xs:totalDigits value="20"/>
                <xs:fractionDigits value="2"/>
            </xs:restriction>
        </xs:simpleContent>
    </xs:complexType>
    <!-- M.CA.SDE.00615: Код страны (Страна. Код) -->
    <xs:element name="CACountryCode" type="casdo:CACountryCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение страны</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="ShortCountryName" type="casdo:Name40Type2">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение стр</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00181: Двухсимвольный код страны (Страна_ Код. Тип) -->
    <xs:complexType name="CACountryCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая символьное обозначение страны, с указанием
                классификатора
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:restriction base="csdo:QualifiedCodeType">
                <xs:pattern value="([А-Я]{40})"/>
            </xs:restriction>
        </xs:simpleContent>
    </xs:complexType>
    <!-- M.CA.SDT.00200: Код валюты (Валюта_ Код. Тип) -->
    <xs:simpleType name="CACurrencyCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая буквенное обозначение валюты или значение
                &quot;000&quot;
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:pattern value="[A-Z]{3}|[0]{3}"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.CA.SDT.00088: Номер документа в журнале регистрации (Документ в журнале регистрации_ Идентификатор. Тип) -->
    <xs:simpleType name="CADocIdType">
        <xs:annotation>
            <xs:documentation>строка символов, идентифицирующая документ в журнале регистрации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:IdentifierType">
            <xs:minLength value="1"/>
            <xs:maxLength value="7"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.CA.SDE.00202: Стоимость (Стоимость_ Величина без указания классификатора валют. Денежная сумма) -->
    <xs:element name="CAInvoiceValueAmount" type="casdo:CAAmountWithCurrencyType">
        <xs:annotation>
            <xs:documentation>Стоимость</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00334: Сумма платежа (Платеж. Денежная сумма) -->
    <xs:element name="CAPaymentAmount" type="casdo:PaymentAmountWithCurrencyType">
        <xs:annotation>
            <xs:documentation>Сумма платежа</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00697: Сумма (Платеж_ Сумма с указанием цифрового кода валюты. Денежная сумма) -->
    <xs:element name="CAPaymentNAmount" type="casdo:PaymentAmountWithNCurrencyType">
        <xs:annotation>
            <xs:documentation>Сумма платежа</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00266: Общая стоимость товара (услуги) (Общая стоимость товара (услуги). Денежная сумма) -->
    <xs:element name="CAProductCostAmount" type="casdo:CAAmountWithCurrencyType">
        <xs:annotation>
            <xs:documentation>Общая стоимость товара (услуги), выраженная в денежной сумме</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDT.00191: Код (Код с указанием справочника (классификатора)_ Код. Тип) -->
    <xs:complexType name="CAQualifiedCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая кодовое обозначение объекта, с указанием справочника
                (классификатора)
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="bdt:CodeType">
                <xs:attribute name="codeListId" type="csdo:ReferenceDataIdType" use="optional">
                    <xs:annotation>
                        <xs:documentation>Обозначение справочника (классификатора), в соответствии с которым указан
                            код
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <!-- M.CA.SDE.00626: Уникальный идентификационный таможенный номер (Уникальный идентификационный таможенный номер. Идентификатор) -->
    <xs:element name="CAUniqueCustomsNumberId" type="casdo:CAUniqueCustomsNumberIdType">
        <xs:annotation>
