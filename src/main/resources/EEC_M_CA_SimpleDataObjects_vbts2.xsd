    <xs:element name="WoodKindName" type="csdo:Name20Type">
        <xs:annotation>
            <xs:documentation>Наименование породы древесины</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.SDE.00211: Сортимент товара (Сортимент товара. Имя) -->
