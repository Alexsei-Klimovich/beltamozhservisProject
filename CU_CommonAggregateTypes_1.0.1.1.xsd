<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cat="urn:CU:CommonAggregateTypes:1.0.1.1"
           xmlns:clt="urn:CU:CommonLeafTypes:1.0.1" targetNamespace="urn:CU:CommonAggregateTypes:1.0.1.1"
           elementFormDefault="qualified" version="1.0.1.0">
    <xs:import namespace="urn:CU:CommonLeafTypes:1.0.1" schemaLocation="CommonLeafTypes_Cust.xsd"/>
    <xs:annotation>
        <xs:documentation>Общие сложные прикладные типы.</xs:documentation>
    </xs:annotation>
    <xs:complexType name="AddressType">
        <xs:annotation>
            <xs:documentation>Адрес организации / физического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="PostalCode" type="clt:PostalCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Почтовый код (индекс)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CountryCode" type="clt:CountryA2CodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код страны в соответствии с классификатором стран мира</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CounryName" type="clt:CountryNameType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Краткое название страны</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Region" type="clt:RegionType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Область (регион, штат, провинция и т.п.)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="City" type="clt:CityNameType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Населенный пункт.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StreetHouse" type="clt:StreetHouseType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Улица, номер дома, номер квартиры (офиса)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AddressLine" type="clt:Text150Type" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Строка неразборного адреса</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AnyDocumentType">
        <xs:annotation>
            <xs:documentation>Контейнер с произвольным содержимым.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:any processContents="strict" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Любое содержимое.</xs:documentation>
                </xs:annotation>
            </xs:any>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AuthoritesDocumentType">
        <xs:annotation>
            <xs:documentation>Документ, подтверждающий полномочия.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cat:DocumentBaseType">
                <xs:sequence>
                    <xs:element name="ComplationAuthorityDate" type="clt:DateCustType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Дата окончания полномочий.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AutoType">
        <xs:annotation>
            <xs:documentation>Сведения об автомобиле</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cat:TransportType">
                <xs:sequence>
                    <xs:element name="Mark" type="clt:MarkCarNameType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Наименование марки в оригинале</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="MarkCode" type="clt:MarkCarCodeType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Код марки</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="OfftakeYear" type="clt:DateYearCustType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Год изготовления транспортного средства</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EngineVolumeQuanity" type="clt:EngineVolumeType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Рабочий объем двигателя, куб. см</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VINID" type="clt:VINIDType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Идентификационный номер (VIN)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="BodyID" type="clt:VINIDType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Идентификационный номер кузова (прицепа), присвоенные и нанесенные на них
                                организацией-изготовителем.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EngineID" type="clt:EngineIDType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Присвоенный организацией - изготовителем идентификационный номер
                                двигателя, нанесенный на блоке двигателя. Идентификационный номер может состоять из
                                групп раздельных цифр, из которых последняя группа, состоящая из двух цифр, указывает
                                год выпуска двигателя
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ChassisID" type="clt:VINIDType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Идентификационный номер шасси (рамы), присвоенные и нанесенные на них
                                организацией-изготовителем.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="OperationTerm" type="clt:Count10Type" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Срок эксплуатации (до 6 цифер)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="TehPassportNumber" type="clt:TehPassportNumber" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Номер тех. паспорта ТС (00 AA 000000)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Run" type="clt:Count10Type" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Пробег автомобиля (число лет эксплуатации)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="CabID" type="clt:VINIDType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Идентификационный номер кабины.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="BankInformationType">
        <xs:annotation>
            <xs:documentation>Информация о банковских счетах</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="BankAccount" type="clt:ForeignAccountType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Номер банковского счета</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BankAccountDescription" type="clt:FreeText250Type" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Описание типа счета: расчетный, валютный,...</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BankName" type="clt:NameType">
                <xs:annotation>
                    <xs:documentation>Название банка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BankMFO" type="clt:BankMFOType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>МФО банка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OKPOID" type="clt:OKPOIDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код банка по ОКПО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BICID" type="clt:BICIDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>БИК банка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SWIFTID" type="clt:SWIFTIDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>SWIFT код банка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CorrAccount" type="clt:ForeignAccountType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер корреспондентского счета банка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TransitCurrencyAccount" type="clt:ForeignAccountType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Валютный счет организации (транзитный)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SpecialTransitCurrencyAccount" type="clt:ForeignAccountType" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Валютный счет организации (специальный транзитный)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="BaseDocType">
        <xs:annotation>
            <xs:documentation>Базовый тип для электронного документа.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="DocumentID" type="clt:DocumentIDType">
                <xs:annotation>
                    <xs:documentation>Уникальный идентификатор документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RefDocumentID" type="clt:DocumentIDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Уникальный идентификатор исходного документа</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ContainerType">
        <xs:annotation>
            <xs:documentation>Транспортные контейнеры.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Identity" type="clt:ContainerIdentityType">
                <xs:annotation>
                    <xs:documentation>Номер контейнера.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CustomsOfficeListType">
        <xs:annotation>
            <xs:documentation>Таможня.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cat:CustomsOfficeType">
                <xs:sequence>
                    <xs:element name="Sequence" type="clt:Count2Type" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Порядковый номер таможенного органа в списке.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CustomsOfficeType">
        <xs:annotation>
            <xs:documentation>Таможенный орган.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CustomsCode">
                <xs:annotation>
                    <xs:documentation>Код таможенного органа в соответствии с классификатором таможенных органов
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="clt:CustomsCodeType">
                        <xs:length value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CustomsOfficialType">
        <xs:annotation>
            <xs:documentation>Сведения о должностном лице (инспекторе) таможенного органа, зафиксировавшем вывоз
                товаров
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="clt:NameType">
                <xs:annotation>
                    <xs:documentation>ФИО должностного лица ТО.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LNPNumber" type="clt:LNPIDType">
                <xs:annotation>
                    <xs:documentation>Номер ЛНП должностного лица ТО.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LNPCustomsCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код таможенного органа на оттиске ЛНП</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="clt:CustomsCodeType">
                        <xs:length value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CustomsType">
        <xs:annotation>
            <xs:documentation>Таможня</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Code" type="clt:CustomsCodeType">
                <xs:annotation>
                    <xs:documentation>Код таможенного органа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OfficeName" type="clt:CustomsOfficeNameType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование таможенного органа</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DeliveryTermsType">
        <xs:annotation>
            <xs:documentation>Условие поставки товаров.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="DeliveryPlace" type="clt:PlaceNameType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Описание / Название географического пункта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DeliveryTermsStringCode" type="clt:IncotermsDeliveryStringCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код условий поставки по Инкотермс.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DimensionsType">
        <xs:annotation>
            <xs:documentation>Габариты</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="DimensionsCode" type="clt:CodeVariable3Type">
                <xs:annotation>
                    <xs:documentation>Код единицы измерения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Lenght" type="clt:Quantity5Type">
                <xs:annotation>
                    <xs:documentation>Длина</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Wight" type="clt:Quantity5Type">
                <xs:annotation>
                    <xs:documentation>Ширина</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Height" type="clt:Quantity5Type">
                <xs:annotation>
                    <xs:documentation>Высота</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DocumentBaseType">
        <xs:annotation>
            <xs:documentation>Базовый класс для указания документов. Наименование, номер, дата.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="DocumentName" type="clt:DocumentNameType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование документа, которым предоставлено обеспечение</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentReference" type="clt:DocumentNumberType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер документа, которым предоставлено обеспечение</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentDate" type="clt:DateCustType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата документа, которым предоставлено обеспечение</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DocumentType">
        <xs:annotation>
            <xs:documentation>Документы</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cat:DocumentBaseType">
                <xs:sequence>
                    <xs:element name="DocumentKind" type="clt:CodeVariable5Type">
                        <xs:annotation>
                            <xs:documentation>Код документа в соответствии с классификатором видов документов и сведений
                                или рабочий объем двигателя транспортного средства
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="EDocumentType">
        <xs:sequence>
            <xs:element name="DocumentID" type="clt:DocumentIDType">
                <xs:annotation>
                    <xs:documentation>Уникальный идентификатор документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RefDocumentID" type="clt:DocumentIDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Уникальный идентификатор исходного документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentDateTime" type="clt:DateTimeCustType">
                <xs:annotation>
                    <xs:documentation>Дата и время формирования документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentVersionNumber" type="clt:Count5Type" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер версии документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IsTest" type="clt:IndicatorType">
                <xs:annotation>
                    <xs:documentation>Признак тестового документа.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ErrorInformationType">
        <xs:annotation>
            <xs:documentation>Информация об ошибке.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ErrorCode" type="clt:Code4Type">
                <xs:annotation>
                    <xs:documentation>Код ошибки.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ErrorDescription" type="clt:FreeStringType">
                <xs:annotation>
                    <xs:documentation>Текстовое описание ошибки.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ErrorSource" type="clt:Text40Type">
                <xs:annotation>
                    <xs:documentation>Источник ошибки</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GTDID5Type">
        <xs:annotation>
            <xs:documentation>Регистрационный номер таможенного документа. Применяется для всех документов имеющих
                структуру номера, аналогичную структуре номера ТД. Последний элемент 5 знаков.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CustomsCode" type="clt:CustomsCodeType">
                <xs:annotation>
                    <xs:documentation>Код таможенного органа, зарегистрировавшего документ.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RegistrationDate" type="clt:DateCustType">
                <xs:annotation>
                    <xs:documentation>Дата регистрации документа.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SerialNumber" type="clt:Code5Type">
                <xs:annotation>
                    <xs:documentation>Порядковый номер документа по журналу регистрации.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GTDID6Type">
        <xs:annotation>
            <xs:documentation>Регистрационный номер таможенного документа. Применяется для всех документов имеющих
                структуру номера, аналогичную структуре номера ТД. Последний элемент 6 знаков.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CustomsCode" type="clt:CustomsCodeType">
                <xs:annotation>
                    <xs:documentation>Код таможенного органа, зарегистрировавшего документ.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RegistrationDate" type="clt:DateCustType">
                <xs:annotation>
                    <xs:documentation>Дата регистрации документа.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SerialNumber" type="clt:Code6Type">
                <xs:annotation>
                    <xs:documentation>Порядковый номер документа по журналу регистрации.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GTDIDType">
        <xs:annotation>
            <xs:documentation>Регистрационный номер таможенного документа. Применяется для всех документов имеющих
                структуру номера, совпадающую с со структурой номера ТД.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CustomsCode">
                <xs:annotation>
                    <xs:documentation>Код таможенного органа, зарегистрировавшего документ.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="clt:CustomsCodeType">
                        <xs:length value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="RegistrationDate" type="clt:DateCustType">
                <xs:annotation>
                    <xs:documentation>Дата регистрации документа.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GTDNumber" type="clt:GTDIDType">
                <xs:annotation>
                    <xs:documentation>Порядковый номер документа по журналу регистрации.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GoodsBaseType">
        <xs:annotation>
            <xs:documentation>Базовый тип описания товара.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
