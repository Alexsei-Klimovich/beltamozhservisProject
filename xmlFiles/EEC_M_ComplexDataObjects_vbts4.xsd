    <xs:element name="CodeListEventDetails" type="ccdo:CodeListEventDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о событии, связанном со справочником (классификатором)</xs:documentation>
        </xs:annotation>
    </xs:element>
