<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ccdo="urn:EEC:M:ComplexDataObjects:vbts4"
           xmlns:csdo="urn:EEC:M:SimpleDataObjects:vbts5" targetNamespace="urn:EEC:M:ComplexDataObjects:vbts4"
           elementFormDefault="qualified" version="0.4.11" xmlns:casdo="urn:EEC:M:CA:SimpleDataObjects:vbts2">
    <xs:import namespace="urn:EEC:M:SimpleDataObjects:vbts5" schemaLocation="EEC_M_SimpleDataObjects_vbts5.xsd"/>
    <xs:import namespace="urn:EEC:M:CA:SimpleDataObjects:vbts2" schemaLocation="EEC_M_CA_SimpleDataObjects_vbts2.xsd"/>
    <!-- M.CDE.00080: Документ об образовании (Документ об образовании. Детали) -->
    <xs:element name="AcademicCertificateDetails" type="ccdo:AcademicCertificateDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о документе об образовании (квалификации, обучении)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00085: Документ об образовании (Документ об образовании_ Детали. Тип) -->
    <xs:complexType name="AcademicCertificateDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие детализированную информацию о документе об образовании
                (квалификации, обучении)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:UnifiedCountryCode"/>
            <xs:element ref="csdo:DocKindName" minOccurs="0"/>
            <xs:element ref="csdo:DocSeriesId" minOccurs="0"/>
            <xs:element ref="csdo:DocId"/>
            <xs:element ref="csdo:DocCreationDate" minOccurs="0"/>
            <xs:element ref="csdo:BusinessEntityId" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Строка, идентифицирующая хозяйствующего субъекта, выдавшего документ
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:BusinessEntityName">
                <xs:annotation>
                    <xs:documentation>Полное наименование хозяйствующего субъекта, выдавшего документ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CDE.00030: Альтернативный адрес (Альтернативный адрес. Детали) -->
    <xs:element name="AdditionalAddressDetails" type="ccdo:AddressDetailsType">
        <xs:annotation>
            <xs:documentation>Адрес, который отличается от основного адреса</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00035: Адрес (Адресные реквизиты_ Детали. Тип) -->
    <xs:complexType name="AddressBaseDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие детализированную информацию об адресе</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:CountryCode" minOccurs="0"/>
            <xs:element ref="csdo:TerritoryCode" minOccurs="0"/>
            <xs:element ref="csdo:PostCode" minOccurs="0"/>
            <xs:element ref="csdo:RegionName" minOccurs="0"/>
            <xs:element ref="csdo:DistrictName" minOccurs="0"/>
            <xs:element ref="csdo:CityName" minOccurs="0"/>
            <xs:element ref="csdo:SettlementName" minOccurs="0"/>
            <xs:element ref="csdo:StreetName" minOccurs="0"/>
            <xs:element ref="csdo:BuildingNumberId" minOccurs="0"/>
            <xs:element ref="csdo:RoomNumberId" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CDE.00001: Адрес (Адрес. Детали) -->
    <xs:element name="AddressDetails" type="ccdo:AddressDetailsType">
        <xs:annotation>
            <xs:documentation>Адрес регистрации</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00001: Адрес (Адрес_ Детали. Тип) -->
    <xs:complexType name="AddressDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие детализированную информацию об адресе</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ccdo:AddressBaseDetailsType">
                <xs:sequence>
                    <xs:element ref="csdo:AddressText" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- M.CDT.00079: Адрес (Адрес_ Детали: вариант 4. Тип) -->
    <xs:complexType name="AddressDetailsV4Type">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие детализированную информацию об адресе</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:AddressKindCode" minOccurs="0"/>
            <xs:element ref="csdo:UnifiedCountryCode" minOccurs="0"/>
            <xs:element ref="csdo:TerritoryCode" minOccurs="0"/>
            <xs:element ref="csdo:RegionName" minOccurs="0"/>
            <xs:element ref="csdo:DistrictName" minOccurs="0"/>
            <xs:element ref="csdo:CityName" minOccurs="0"/>
            <xs:element ref="csdo:SettlementName" minOccurs="0"/>
            <xs:element ref="csdo:StreetName" minOccurs="0"/>
            <xs:element ref="csdo:BuildingNumberId" minOccurs="0"/>
            <xs:element ref="csdo:RoomNumberId" minOccurs="0"/>
            <xs:element ref="csdo:PostCode" minOccurs="0"/>
            <xs:element ref="csdo:PostOfficeBoxId" minOccurs="0"/>
            <xs:element ref="csdo:AddressText" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CDE.00076: Адрес (Адрес: вариант 4. Детали) -->
    <xs:element name="AddressV4Details" type="ccdo:AddressDetailsV4Type">
        <xs:annotation>
            <xs:documentation>Адрес объекта</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDE.00081: XML-документ (XML-документ. Детали) -->
    <xs:element name="AnyDetails" type="ccdo:AnyDetailsType">
        <xs:annotation>
            <xs:documentation>XML-документ произвольной структуры</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00086: XML-документ (XML-документ_ Детали. Тип) -->
    <xs:complexType name="AnyDetailsType">
        <xs:annotation>
            <xs:documentation>XML-документ произвольной структуры</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:any namespace="##any" processContents="strict" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Содержимое XML-документа произвольной структуры</xs:documentation>
                </xs:annotation>
            </xs:any>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CDE.00055: Уполномоченный орган государства-члена (Уполномоченный орган государства-члена. Детали) -->
    <xs:element name="AuthorityDetails" type="ccdo:AuthorityDetailsType">
        <xs:annotation>
            <xs:documentation>Совокупность сведений об органе государственной власти государства-члена либо об
                уполномоченной им организации
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00025: Уполномоченный орган государства-члена (Уполномоченный орган государства-члена_ Детали. Тип) -->
    <xs:complexType name="AuthorityDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие детализированную информацию об органе государственной власти
                государства-члена либо об уполномоченной им организации
            </xs:documentation>
        </xs:annotation>
