            <xs:element ref="csdo:CountryCode" minOccurs="0"/>
            <xs:element ref="csdo:AuthorityId" minOccurs="0"/>
            <xs:element ref="csdo:AuthorityName" minOccurs="0"/>
            <xs:element ref="csdo:AuthorityBriefName" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CDE.00060: Банковский счет (Банковский счет. Детали) -->
    <xs:element name="BankAccountDetails" type="ccdo:BankAccountDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о банковском счете</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00066: Банковский счет (Банковский счет_ Детали. Тип) -->
    <xs:complexType name="BankAccountDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие детализированную информацию о банковском счете
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:BankAccountId" maxOccurs="unbounded"/>
            <xs:element ref="ccdo:BankDetails"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CDE.00061: Банк (Банк. Детали) -->
    <xs:element name="BankDetails" type="ccdo:BankDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о банке</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00067: Банк (Банк_ Детали. Тип) -->
    <xs:complexType name="BankDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие детализированную информацию о банке</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:BusinessEntityName">
                <xs:annotation>
                    <xs:documentation>Полное наименование банка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:UnifiedBankId"/>
            <xs:element ref="csdo:BankCorrAccountId" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CDE.00011: Банковские реквизиты (Банковские реквизиты. Детали) -->
    <xs:element name="BankingDetails" type="ccdo:BankingDetailsType">
        <xs:annotation>
            <xs:documentation>Совокупность банковских реквизитов юридического или физического лица</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CDT.00011: Банковские реквизиты (Банковские реквизиты_ Детали. Тип) -->
    <xs:complexType name="BankingDetailsType">
        <xs:annotation>
            <xs:documentation>Совокупность банковских реквизитов юридического или физического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:BankId"/>
            <xs:element ref="csdo:BankName" minOccurs="0"/>
            <xs:element ref="csdo:BankCorrAccountId" minOccurs="0"/>
            <xs:element ref="csdo:PayeeAccountId"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CDT.00061: Хозяйствующий субъект (Хозяйствующий субъект_ Детали. Тип) -->
    <xs:complexType name="BusinessEntityDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие детализированную информацию о хозяйствующем субъекте
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:UnifiedCountryCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Кодовое обозначение страны регистрации хозяйствующего субъекта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:BusinessEntityName" minOccurs="0"/>
            <xs:element ref="csdo:BusinessEntityBriefName" minOccurs="0"/>
            <xs:element ref="csdo:BusinessEntityTypeCode" minOccurs="0"/>
            <xs:element ref="csdo:BusinessEntityTypeName" minOccurs="0"/>
            <xs:element ref="csdo:BusinessEntityId" minOccurs="0"/>
            <xs:element ref="csdo:UniqueCustomsNumberId" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Уникальный идентификационный номер хозяйствующего субъекта, предназначенный для
                        целей таможенного контроля
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:TaxpayerId" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор хозяйствующего субъекта в реестре налогоплательщиков страны
                        регистрации налогоплательщика
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:TaxRegistrationReasonCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код, идентифицирующий причину постановки хозяйствующего субъекта на налоговый учет
                        в Российской Федерации
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="ccdo:SubjectAddressDetails" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Адрес хозяйствующего субъекта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="ccdo:CommunicationDetails" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Контактный реквизит хозяйствующего субъекта</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
