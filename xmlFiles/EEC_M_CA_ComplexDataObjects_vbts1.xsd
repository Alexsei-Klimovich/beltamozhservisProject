<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cacdo="urn:EEC:M:CA:ComplexDataObjects:vbts1"
           xmlns:casdo="urn:EEC:M:CA:SimpleDataObjects:vbts2" xmlns:ccdo="urn:EEC:M:ComplexDataObjects:vbts4"
           xmlns:csdo="urn:EEC:M:SimpleDataObjects:vbts5" targetNamespace="urn:EEC:M:CA:ComplexDataObjects:vbts1"
           elementFormDefault="qualified" version="1.5.2">
    <xs:import namespace="urn:EEC:M:CA:SimpleDataObjects:vbts2" schemaLocation="EEC_M_CA_SimpleDataObjects_vbts2.xsd"/>
    <xs:import namespace="urn:EEC:M:ComplexDataObjects:vbts4" schemaLocation="EEC_M_ComplexDataObjects_vbts4.xsd"/>
    <xs:import namespace="urn:EEC:M:SimpleDataObjects:vbts5" schemaLocation="EEC_M_SimpleDataObjects_vbts5.xsd"/>
    <!-- M.CA.CDE.00466: Свидетельство уполномоченного экономического оператора (Уполномоченный экономический оператор_ Номер свидетельства о включении в реестр. Детали) -->
    <xs:element name="AEORegistryIdDetails" type="cacdo:AEORegistryIdDetailsType">
        <xs:annotation>
            <xs:documentation>Номер свидетельства о включении в реестр уполномоченных экономических операторов
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.00468: Номер свидетельства о включении в реестр уполномоченных экономических операторов (Номер свидетельства о включении в реестр уполномоченных экономических операторов_ Детали. Тип) -->
    <xs:complexType name="AEORegistryIdDetailsType">
        <xs:annotation>
            <xs:documentation>компоненты, представляющие сведения о номере свидетельства о включении юридического лица в
                реестр уполномоченных экономических операторов
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:UnifiedCountryCode" minOccurs="0"/>
            <xs:element ref="csdo:DocId">
                <xs:annotation>
                    <xs:documentation>номер свидетельства, присвоенный уполномоченным органом государства-члена
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="casdo:AEORegistryKindCode"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.00650: Место хранения уполномоченного экономического оператора (Место хранения_ уполномоченный экономический оператор. Детали) -->
    <xs:element name="AEOStorageFacilityDetails" type="cacdo:AEOStorageFacilityDetailsType">
        <xs:annotation>
            <xs:documentation>сведения о сооружениях, помещениях (частях помещений) и (или) открытых площадках (частей
                открытых площадок) уполномоченного экономического оператора, предназначенных для использования или
                используемых для временного хранения товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.00650: Место хранения уполномоченного экономического оператора (Место хранения уполномоченного экономического оператора_ Детали. Тип) -->
    <xs:complexType name="AEOStorageFacilityDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения о сооружениях, помещениях (частях помещений) и (или)
                открытых площадках (частей открытых площадок) уполномоченного экономического оператора, предназначенных
                для использования или используемых для временного хранения товаров
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="casdo:PlaceName">
                <xs:annotation>
                    <xs:documentation>наименование места хранения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="ccdo:AddressDetails">
                <xs:annotation>
                    <xs:documentation>фактический адрес места хранения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="casdo:TotalAreaMeasure">
                <xs:annotation>
                    <xs:documentation>площадь места хранения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="cacdo:CustomsControlZoneDetails" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.00186: Регистрационный номер акта таможенного досмотра (таможенного осмотра) (Документ, подтверждающий проведение таможенного досмотра (таможенного осмотра) товаров_ регистрационный номер. Детали) -->
    <xs:element name="ActInspectionDocIdDetails" type="cacdo:CustomsDocumentIdDetailsType">
        <xs:annotation>
            <xs:documentation>Регистрационный номер документа, подтверждающего проведение таможенного досмотра
                (таможенного осмотра) товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00568: Количество товара в единице измерения, отличной от основной и дополнительной (Товар_ Количество в единицах, отличных от дополнительных. Детали) -->
    <xs:element name="AddGoodsMeasureDetails" type="cacdo:GoodsMeasureDetailsType">
        <xs:annotation>
            <xs:documentation>Количество товара в единице измерения, отличной от основной и дополнительной
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00194: Дополнение к договору поручительства (Договор поручительства_ Дополнение к договору. Детали) -->
    <xs:element name="AddSuretyContractDetails" type="ccdo:DocDetailsV4Type">
        <xs:annotation>
            <xs:documentation>Сведения о дополнении к договору поручительства</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00518: Поправки к стоимости сделки в сторону увеличения (Поправки к стоимости сделки в сторону увеличения. Детали) -->
    <xs:element name="AdditionsAdjustmentsDetails" type="cacdo:IdenticalGoodsCorrectionDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о поправках к стоимости сделки с идентичными или однородными товарами, если
                расходы в отношении идентичных или однородных товаров меньше расходов в отношении ввозимых товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.01104: Сведения о заходе в порт на зараженной территории (Сведения о заходе в порт на зараженной территории. Детали) -->
    <xs:element name="AffectedPortDetails" type="cacdo:PIVPortDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о заходе в порт на зараженной территории</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.01131: Агент (Агент. Детали) -->
    <xs:element name="AgentDetails" type="cacdo:AgentDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения об агенте</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.01131: Агент (Агент_ Детали. Тип) -->
    <xs:complexType name="AgentDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения об агенте</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="cacdo:CASubjectDetailsV2Type">
                <xs:sequence>
                    <xs:element ref="cacdo:AgentRepresentativeDetails" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- M.CA.CDE.01130: Представитель агента (Агент_ Представитель. Детали) -->
    <xs:element name="AgentRepresentativeDetails" type="ccdo:OfficerDetailsType">
        <xs:annotation>
            <xs:documentation>сведения о лице, являющимся представителем агента</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00346: Величина припуска (Величина припуска. Детали) -->
    <xs:element name="AllowanceDetails" type="ccdo:UnifiedOverallDimensionDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о величине припуска по длине, ширине и высоте</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.00124: Остаток квоты в стоимостном выражении (Квота_ Остаток в стоимостном выражении_ Детали. Тип) -->
    <xs:complexType name="AmountQuotaDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения об остатке квоты в стоимостном выражении</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="casdo:QuotaRemainderAmount"/>
            <xs:element ref="casdo:CurrencyName" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.01105: Сведения о противоэпидемическом имуществе на борту (Предварительная информация о товарах, перемещаемых водным транспортом_ Противоэпидемическое имущество на борту. Детали) -->
    <xs:element name="AntiepidemicEquipmentDetails" type="cacdo:AntiepidemicEquipmentDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о противоэпидемическом имуществе на борту судна</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.01103: Сведения о противоэпидемическом имуществе на борту судна (Судно_ Сведения о противоэпидемическом имуществе на борту_ Детали. Тип) -->
    <xs:complexType name="AntiepidemicEquipmentDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения о противоэпидемическом имуществе на борту судна
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="casdo:AntiepidemicEquipmentIndicator"/>
            <xs:element ref="csdo:DescriptionText" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Описание противоэпидемического имущества</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.01106: Противоэпидемическая готовность судна (Предварительная информация о товарах, перемещаемых водным транспортом_ Противоэпидемическая готовность водного судна. Детали) -->
    <xs:element name="AntiepidemicReadyDetails" type="cacdo:AntiepidemicReadyDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о противоэпидемической готовности судна</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.01104: Противоэпидемическая готовность транспортного средства (Транспортное средство_ Противоэпидемическая готовность_ Детали. Тип) -->
    <xs:complexType name="AntiepidemicReadyDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения о противоэпидемической готовности транспортного
                средства
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="cacdo:AntiepidemicEquipmentDetails"/>
            <xs:element ref="cacdo:CrewTrainingDetails">
                <xs:annotation>
                    <xs:documentation>Сведения противоэпидемической подготовке экипажа</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.00390: Подробности соответствия стоимости сделки и проверочной величины (Подробности соответствия стоимости сделки и проверочной величины. Детали) -->
    <xs:element name="ApproximateValueDetails" type="cacdo:VerificationValueDetailsType">
        <xs:annotation>
            <xs:documentation>Подробные сведения о соответствии стоимости сделки и проверочной величины
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00130: Адрес места совершения таможенных операций в месте прибытия (убытия) товаров (Место прибытия (убытия) товаров_ Адрес места совершения таможенных операций. Детали) -->
    <xs:element name="ArrivalDepartureAddressDetails" type="cacdo:PointAddressDetailsType">
        <xs:annotation>
            <xs:documentation>сведения об адресе места совершения таможенных операций в месте прибытия (убытия)
                товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.00130: Адрес или географические координаты места объекта (Адрес или географические координаты места объекта_ Детали. Тип) -->
    <xs:complexType name="ArrivalDepartureAddressDetailsType">
        <xs:annotation>
            <xs:documentation>компоненты, представляющие сведения об адресе или географических координатах
                местонахождения объекта
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="ccdo:AddressDetails" minOccurs="0"/>
            <xs:element ref="ccdo:GeoCoordinateDetails" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.00173: Транспортное средство при прибытии (отправлении) (Перевозка_ Транспортное средство при прибытии (отправлении). Детали) -->
    <xs:element name="ArrivalDepartureTransportDetails" type="cacdo:DeclarationTransportMeansDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о транспортных средствах при прибытии (отправлении)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00234: Номер подтверждения о прибытии (Номер подтверждения о прибытии. Детали) -->
    <xs:element name="ArrivalDocumentIdDetails" type="cacdo:CustomsDocumentIdDetailsType">
        <xs:annotation>
            <xs:documentation>Номер подтверждения о прибытии</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00001: Таможенный орган в месте прибытия (Место прибытия (убытия) товаров_ Таможенный орган. Детали) -->
    <xs:element name="ArriveCustomsDetails" type="cacdo:ArriveCustomsDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о таможенном органе, совершающем таможенные операции, в месте прибытия (убытия)
                товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.00001: Таможенный орган, осуществляющий таможенные операции в месте прибытия (убытия) товаров (Таможенный орган, осуществляющий таможенные операции в месте прибытия (убытия) товаров_ Детали. Тип) -->
    <xs:complexType name="ArriveCustomsDetailsType">
        <xs:annotation>
            <xs:documentation>компоненты, представляющие сведения о таможенном органе, совершающем таможенные операции,
                в месте прибытия (убытия) товаров
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:CustomsOfficeCode">
                <xs:annotation>
                    <xs:documentation>Код таможенного органа в соответствии с классификаторами таможенных органов,
                        применяемыми в государствах-членах (третий элемент кодового обозначения места прибытия (убытия)
                        товаров)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:CustomsOfficeName" minOccurs="0"/>
            <xs:element ref="csdo:TransportModeCode" minOccurs="0"/>
            <xs:element ref="casdo:TransportModeName" minOccurs="0"/>
            <xs:element ref="cacdo:ArrivePointLocationDetails"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.00002: Сведения о месте прибытия (убытия) товаров (Место прибытия (убытия) товаров. Детали) -->
    <xs:element name="ArrivePointDetails" type="cacdo:ArrivePointDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о месте прибытия (убытия) товаров в реестре мест прибытия (убытия) товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.00002: Место прибытия (убытия) товаров (Место прибытия (убытия) товаров_ Детали. Тип) -->
    <xs:complexType name="ArrivePointDetailsType">
        <xs:annotation>
            <xs:documentation>компоненты, представляющие сведения о месте прибытия (убытия) товаров</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="casdo:ArrivePointPADCode"/>
            <xs:element ref="csdo:ProvidedInformationCountryCode">
                <xs:annotation>
                    <xs:documentation>Кодовое обозначение страны, предоставившей информацию (второй элемент кодового
                        обозначения места прибытия (убытия) товаров)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="casdo:ArrivePointName" minOccurs="0"/>
            <xs:element ref="casdo:BorderCheckPointCode" minOccurs="0"/>
            <xs:element ref="casdo:BorderCheckPointName" minOccurs="0"/>
            <xs:element ref="cacdo:ArriveCustomsDetails"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.00004: Место прибытия (убытия) товаров. Место совершения таможенных операций (Место прибытия (убытия) товаров_ Место совершения таможенных операций. Детали) -->
    <xs:element name="ArrivePointLocationDetails" type="cacdo:ArrivePointLocationDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о местах совершения таможенных операций в месте прибытия (убытия) товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.00003: Место совершения таможенных операций в месте прибытия (убытия) товаров (Место совершения таможенных операций в месте прибытия (убытия) товаров_ Детали. Тип) -->
    <xs:complexType name="ArrivePointLocationDetailsType">
        <xs:annotation>
            <xs:documentation>компоненты, представляющие сведения о местах совершения таможенных операций в месте
                прибытия (убытия) товаров
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="casdo:CustomsOperationText" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="casdo:ArrivePointSequenceCode"/>
            <xs:element ref="cacdo:PointLocationAddressDetails"/>
            <xs:element ref="cacdo:ArrivePointLocationWorkDetails"/>
            <xs:element ref="ccdo:ResourceItemStatusDetails"/>
            <xs:element ref="csdo:ActivityStartDate" minOccurs="0"/>
            <xs:element ref="csdo:ActivityEndDate" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.00005: Режим работы подразделения таможенного органа, совершающего таможенные операции в месте прибытия (убытия) товаров (Место прибытия (убытия) товаров_ Режим работы. Детали) -->
    <xs:element name="ArrivePointLocationWorkDetails" type="cacdo:ArrivePointLocationWorkDetailsType">
        <xs:annotation>
            <xs:documentation>информация о режиме работы подразделения таможенного органа, совершающего таможенные
                операции в месте прибытия (убытия) товаров
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDT.00004: Режим работы подразделения таможенного органа, совершающего таможенные операции в месте прибытия (убытия) товаров (Режим работы места прибытия (убытия) товаров_ Детали. Тип) -->
    <xs:complexType name="ArrivePointLocationWorkDetailsType">
        <xs:annotation>
            <xs:documentation>компоненты, представляющие сведения о режиме работы подразделения таможенного органа,
                совершающего таможенные операции в месте прибытия (убытия) товаров
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="casdo:WorkTimeCode"/>
            <xs:element ref="casdo:WorkTimeDescriptionText" minOccurs="0"/>
            <xs:element ref="cacdo:WorkScheduleDetails" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.CA.CDE.00118: Таможенный орган на границе (Таможенный орган_ Таможенный орган на границе. Детали) -->
    <xs:element name="BorderCustomsOfficeDetails" type="ccdo:CustomsOfficeDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о таможенном органе на границе</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00158: Транспортное средство на границе (Перевозка_ Транспортное средство на границе. Детали) -->
    <xs:element name="BorderTransportDetails" type="cacdo:DeclarationTransportMeansDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о транспортных средствах на границе</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00252: Транспортное средство на границе (Транспортное средство: Транспортное средство на границе - устаревший вариант. Детали) -->
    <xs:element name="BorderTransportsDetails" type="cacdo:CATransportMeansDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о транспортном средстве на границе</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.CA.CDE.00100: Реквизиты обособленного подразделения юридического лица (Юридическое лицо (организация)_ Реквизиты обособленного подразделения. Детали) -->
