<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:clt="urn:CU:CommonLeafTypes:1.0.1"
           xmlns:CategoryCust="urn:CU:Categories:1.0.0" targetNamespace="urn:CU:CommonLeafTypes:1.0.1"
           elementFormDefault="qualified" version="1.0.1.0">
    <xs:import namespace="urn:CU:Categories:1.0.0" schemaLocation="Categories_Cust.xsd"/>
    <xs:annotation>
        <xs:documentation>Общие простые прикладные типы</xs:documentation>
    </xs:annotation>
    <xs:simpleType name="AbsenceKindType">
        <xs:annotation>
            <xs:documentation>Идентификатор. Текстовый. Один из списка: "Lost", "Destroyed", "LostAndDestroyed".
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="CategoryCust:IdentifierCategoriesCust">
            <xs:enumeration value="LOST"/>
            <xs:enumeration value="DESTROYED"/>
            <xs:enumeration value="LOST_AND_DESTROYED"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AccountType">
        <xs:annotation>
            <xs:documentation>Число. Номер банковского счета. 0 знаков после запятой. 20 знаков. Используется для
                указания номера счета в российских банках.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="CategoryCust:IdentifierCategoriesCust">
            <xs:pattern value="\d{20}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AmountType">
        <xs:annotation>
            <xs:documentation>Число. Количество денежных единиц. Стоимость. От 0. Всего 16 цифр из них до 2х знаков
                после запятой.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="CategoryCust:NumericCategoriesCust">
            <xs:fractionDigits value="2"/>
            <xs:totalDigits value="16"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ArchStatusCodeType">
        <xs:annotation>
            <xs:documentation>Статус документа в архиве. Текст. До 50 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="CategoryCust:TextCategoriesCust">
            <xs:minLength value="1"/>
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ArchiveDeclarantIDType">
        <xs:annotation>
            <xs:documentation>Идентификатор пользователя электронного архива декларанта. Текстовая строка. До 36
                символов.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="CategoryCust:IdentifierCategoriesCust">
            <xs:minLength value="1"/>
            <xs:maxLength value="36"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ArchiveDocumentIDType">
        <xs:annotation>
            <xs:documentation>Идентификатор документа, присвоенный электроннвм архивом декларанта. Текстовая строка. До
                36 символов.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="CategoryCust:IdentifierCategoriesCust">
            <xs:minLength value="1"/>
            <xs:maxLength value="36"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ArchiveIDType">
        <xs:annotation>
            <xs:documentation>Идентификатор архива декларанта. Текстовая строка. До 36 символов.</xs:documentation>
